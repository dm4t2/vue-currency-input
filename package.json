{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docs:dev": "turbo run dev --filter=docs",
    "docs:build": "turbo run build --filter=docs"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "turbo": "^2.5.3"
  },
  "packageManager": "pnpm@9.15.9"
}
